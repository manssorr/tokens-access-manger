# Production Environment Variables for Server
# Copy this to .env.production and update with your actual values

# Server Port (Render will set this automatically)
PORT=10000

# Node Environment
NODE_ENV=production

# CORS Origins (comma-separated)
# Add your Render frontend URL here after deployment
# Example: CORS_ORIGINS=https://your-app.onrender.com
CORS_ORIGINS=https://your-frontend-url.onrender.com

# Frontend URL (for reference)
FRONTEND_URL=https://your-frontend-url.onrender.com
